{
    "builds": [
        {
            "src": "backend/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/wsgi.py"
        },
        {
            "src": "/admin/(.*)",
            "dest": "backend/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}