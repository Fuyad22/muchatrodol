{
    "builds": [
        {
            "src": "backend/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ]
}